<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.preseason.Selection">
   <children>
      <Button fx:id="addPlayerBtn" layoutX="25.0" layoutY="208.0" mnemonicParsing="false" onAction="#changeScreenToAddPlayer" prefHeight="170.0" prefWidth="213.0" text="Add player" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="addTeamBtn" layoutX="273.0" layoutY="33.0" mnemonicParsing="false" onAction="#changeScreenToTeam" prefHeight="156.0" prefWidth="213.0" text="Add team" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="24.0" />
         </font>
      </Button>
      <Button fx:id="addMatchBtn" layoutX="25.0" layoutY="33.0" mnemonicParsing="false" onAction="#changeScreenToAddMatch" prefHeight="156.0" prefWidth="213.0" text="Add match" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="transferPlayerBtn" layoutX="273.0" layoutY="208.0" mnemonicParsing="false" onAction="#changeScreenToTransferPlayer" prefHeight="170.0" prefWidth="213.0" text="Transfer player" textAlignment="CENTER">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button fx:id="logOutBtn1" layoutX="532.0" layoutY="19.0" mnemonicParsing="false" onAction="#changeScreenSeason" text="Back" />
   </children>
</AnchorPane>
