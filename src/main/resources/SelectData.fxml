<AnchorPane prefHeight="800.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.SelectData">
   <children>
      <Text fx:id="selectData" layoutX="440.0" layoutY="64.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Select Data" wrappingWidth="121.0">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <Button fx:id="logOutBtn" layoutX="915.0" layoutY="23.0" mnemonicParsing="false" onAction="#changeScreen" text="Log out" />
      <Button fx:id="viewDatabaseBtn" layoutX="831.0" layoutY="455.0" mnemonicParsing="false" onAction="#whatKindOfDataIsSelected" text="View Database" />
      <Text fx:id="writeName" layoutX="78.0" layoutY="480.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Write surname:" wrappingWidth="173.0">
         <font>
            <Font size="24.0" />
         </font>
      </Text>
      <TextField fx:id="surname" layoutX="288.0" layoutY="458.0" />
      <Button fx:id="backBtn" layoutX="922.0" layoutY="747.0" mnemonicParsing="false" onAction="#changeScreenBack" text="Back" />
      <ListView fx:id="listView" layoutX="41.0" layoutY="563.0" prefHeight="200.0" prefWidth="860.0" />
