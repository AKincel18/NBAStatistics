<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.viewer.DataViewer">
   <children>
      <Button fx:id="logOutBtn" layoutX="544.0" layoutY="24.0" mnemonicParsing="false" onAction="#changeScreen" text="Back" />
      <Button fx:id="viewDatabaseBtn" layoutX="25.0" layoutY="33.0" mnemonicParsing="false" onAction="#changeScreenToSelectData" prefHeight="158.0" prefWidth="213.0" text="Player's achievements" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="25.0" />
         </font></Button>
      <Button fx:id="timetableButton" layoutX="273.0" layoutY="208.0" mnemonicParsing="false" onAction="#changeScreenToTimetable" prefHeight="156.0" prefWidth="213.0" text="Timetable" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button layoutX="273.0" layoutY="33.0" mnemonicParsing="false" onAction="#changeScreentoListOfTop10Players" prefHeight="156.0" prefWidth="213.0" text="List of top 10 shooters" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button layoutX="25.0" layoutY="208.0" mnemonicParsing="false" onAction="#changeScreenToListOfTeamPlayers" prefHeight="156.0" prefWidth="213.0" text="List of team players" textAlignment="CENTER" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
