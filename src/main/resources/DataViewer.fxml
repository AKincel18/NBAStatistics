<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #002B5E;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.viewer.DataViewer">
   <children>
      <Button fx:id="logOutBtn" layoutX="544.0" layoutY="24.0" mnemonicParsing="false" onAction="#changeScreen" prefHeight="32.0" prefWidth="66.0" style="-fx-background-color: #B8C4CA;" text="Back" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0" />
      <Button fx:id="viewDatabaseBtn" layoutX="86.0" layoutY="69.0" mnemonicParsing="false" onAction="#changeScreenToSelectData" prefHeight="144.0" prefWidth="214.0" style="-fx-background-color: #00538C;" text="Player's achievements" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="25.0" />
         </font></Button>
      <Button fx:id="timetableButton" layoutX="325.0" layoutY="223.0" mnemonicParsing="false" onAction="#changeScreenToTimetable" prefHeight="144.0" prefWidth="214.0" style="-fx-background-color: #00538C;" text="Timetable" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button layoutX="325.0" layoutY="69.0" mnemonicParsing="false" onAction="#changeScreentoListOfTop10Players" prefHeight="144.0" prefWidth="214.0" style="-fx-background-color: #00538C;" text="List of top 10 shooters" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <Button layoutX="86.0" layoutY="223.0" mnemonicParsing="false" onAction="#changeScreenToListOfTeamPlayers" prefHeight="144.0" prefWidth="214.0" style="-fx-background-color: #00538C;" text="List of team players" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font size="25.0" />
         </font>
      </Button>
      <ImageView fx:id="helpBtn" fitHeight="46.0" fitWidth="43.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#helpClicked" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
