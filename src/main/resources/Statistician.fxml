<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.statistician.Statistician">
   <children>
      <ComboBox fx:id="matchesComboBox" layoutX="132.0" layoutY="141.0" prefHeight="25.0" prefWidth="337.0" />
      <Button fx:id="buttonOK" layoutX="284.0" layoutY="248.0" mnemonicParsing="false" onAction="#onClickButtonOK" text="OK" />
       <Text fx:id="choiceText" layoutX="210.0" layoutY="111.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choice match">
           <font>
               <Font size="30.0" />
           </font>
       </Text>
      <Button fx:id="btnBack" layoutX="521.0" layoutY="27.0" mnemonicParsing="false" onAction="#changeScreen" text="Back" />
      <ImageView fx:id="helpBtn" fitHeight="46.0" fitWidth="43.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#helpClicked" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
