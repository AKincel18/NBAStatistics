<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #002B5E;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.statistician.Statistician">
   <children>
      <ComboBox fx:id="matchesComboBox" layoutX="131.0" layoutY="144.0" prefHeight="25.0" prefWidth="337.0" />
      <Button fx:id="buttonOK" layoutX="284.0" layoutY="253.0" mnemonicParsing="false" onAction="#onClickButtonOK" style="-fx-background-color: #00538C;" text="OK" textFill="WHITE" />
       <Text fx:id="choiceText" fill="WHITE" layoutX="209.0" layoutY="92.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Choice match">
           <font>
               <Font size="30.0" />
           </font>
       </Text>
      <Button fx:id="btnBack" layoutX="521.0" layoutY="27.0" mnemonicParsing="false" onAction="#changeScreen" style="-fx-background-color: #B8C4CA;" text="Back" AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="20.0" />
      <ImageView fx:id="helpBtn" fitHeight="46.0" fitWidth="43.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#helpClicked" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
