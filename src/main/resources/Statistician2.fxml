<AnchorPane onKeyPressed="#onKeyPressed" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.statistician.Statistician2">
    <children>
        <Button layoutX="726.0" layoutY="26.0" mnemonicParsing="false" onAction="#changeScreenBack" text="Back" />
