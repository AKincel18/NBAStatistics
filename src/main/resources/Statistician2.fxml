            <font>
                <Font size="20.0" />
            </font>
        </Text>
        <Text fx:id="awayTeamName" layoutX="446.0" layoutY="262.0" strokeType="OUTSIDE" strokeWidth="0.0" text="AWAY TEAM" textAlignment="CENTER" visible="true">
