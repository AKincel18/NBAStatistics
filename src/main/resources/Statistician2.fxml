                <Font size="20.0" />
            </font>
        </Text>
      <ListView fx:id="positionsChoice" layoutX="22.0" layoutY="195.0" prefHeight="117.0" prefWidth="200.0" visible="true" />
      <Text fx:id="homeTeamName" layoutX="445.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="HOME TEAM" textAlignment="CENTER" visible="true">
