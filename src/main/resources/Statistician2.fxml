        <Button fx:id="confirmSquad" layoutX="359.0" layoutY="423.0" mnemonicParsing="false" onAction="#onClickButtonConfirm" prefHeight="57.0" prefWidth="96.0" text="Confirm" textAlignment="CENTER" visible="true">
