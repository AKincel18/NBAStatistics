<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nba_statistics.controllers.viewer.ListOfTop10Shooters">
   <children>
      <ComboBox fx:id="seasonComboBox" layoutX="118.0" layoutY="20.0" prefHeight="50.0" prefWidth="364.0" promptText="Select season" />
      <Button fx:id="selectListButton" layoutX="510.0" layoutY="57.0" mnemonicParsing="false" onAction="#selectListTopTenSchooters" prefHeight="26.0" prefWidth="66.0" text="Select" textAlignment="CENTER" wrapText="true" />
      <Button fx:id="backButton" layoutX="510.0" layoutY="20.0" mnemonicParsing="false" onAction="#changeScreenBack" prefHeight="26.0" prefWidth="66.0" text="Back" textAlignment="CENTER" wrapText="true" />
      <ListView fx:id="topTenListView" layoutX="111.0" layoutY="91.0" prefHeight="295.0" prefWidth="378.0" />
      <ImageView fx:id="helpBtn" fitHeight="46.0" fitWidth="43.0" layoutX="14.0" layoutY="14.0" onMouseClicked="#helpClicked" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
